<div class="wrapper">
  <div class="container">
    <div class="page-header">
      <h1>Edit <%=@team.name %></h1>
    </div>
    <div class="row">
      <div class="col-xs-12 .col-sm-6 .col-md-8">
        <%=form_for @team, url: team_path(@team), html: { multipart: true } do |f| %>
          <%= render partial: 'form', locals: { f: f } %>
          <div class="centered-wide-form">
            <p class="submit"><%=f.submit 'Update Team' %></p>
          </div>  
        <% end %>
      </div>
    </div>
  </div>
</div>