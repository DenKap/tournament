<div class="team-form">
  <div class="wrapper">
    <div class="page-header"><h1>Create Your Team</h1></div>
    <div class="add-form">
      <%=form_for @team, url: teams_path(@team), html: { multipart: true } do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
        <div class="centered-wide-form">
          <p class="submit"><%=f.submit 'Create Team' %></p>
        </div> 
      <% end %>
    </div>
  </div>
</div>
